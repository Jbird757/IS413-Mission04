@{
    ViewData["Title"] = "Grade Calculator";
}



<h2>Grade Calculator</h2> <!--Exactly what it says-->
<form>
    <label>Assignments: </label>
    <input id="assign" /><br />
    <label>Group Projects: </label>
    <input id="group" /><br />
    <label>Quizzes</label>
    <input id="quiz" /><br />
    <label>Midterm: </label>
    <input id="mt" /><br />
    <label>Final Exam: </label>
    <input id="final" /><br />
    <label>INTEX: </label>
    <input id="intex" /><br />
    <input id="button_send" type="button" value="Calculate" />
</form>

<p>Grade:</p>
<!--Calculated value from js-->
<p id="result"></p>

<script src="js/calculator.js"></script>